public class Point {
  
  private float x;
  private float y;
  
  
  /**
   * Constructor
   */
  Point(float _x, float _y) {
    x = _x;
    y = _y;
  }
  
  /**
   * Get the x property.
   */
  public float getX() {
    return x;
  }
  /**
   * Set the x property.
   */
  public void setX(float _x) {
    x = _x;
  }
  /**
   * Get the y property.
   */
  public float getY() {
    return y;
  }
  /**
   * Set the y property.
   */
  public void setY(float _y) {
    y = _y;
  }
}
